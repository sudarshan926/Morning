package Forloop.com;

public class ForLoop {

	public static void main(String[] args) throws Exception{
		
		
		
			int a=10/0;
       System.out.println(a);

		for(int i=2;i<=10;i++)
		{
			int v=0;
			for(int j=2;j<i;j++)
			{
				if(i%j==0)
				{
					v++;
				}
			}
			if(v==0)
			{
				System.out.print(i);
			}
		}
	}

}
