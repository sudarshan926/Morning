package com.java8.in;

interface C{
	static void add(int a, int b) {
		System.out.println("Addition :"+(a+b));
	}
}
interface D{
	static void add(int a, int b) {
		System.out.println("Addition : class D"+(a+b));
	}	
}
public class StaticMethods implements C,D {
	static void add() {
		System.out.println("Addition : class D");
	}
	
	public static void main(String[] args) {
		StaticMethods s=new StaticMethods();
		s.add();
		C.add(20,30);
		D.add(30,40);
	}

}
