package multithreading;
class Demo  extends Thread {
	
	public void run() {
		for (int i = 0; i <= 50; i++) {
			System.out.println("A" + i);
		}
	}
}

class Demo1 implements Runnable{

	public void run() {
		for (int i = 0; i <= 50; i++) {
			System.out.println("A" + i);
		}
	}	
}
public class Threading {

	public static void main(String[] args) {
		Demo d=new Demo();
		Thread t=new Thread();
		
		}
}